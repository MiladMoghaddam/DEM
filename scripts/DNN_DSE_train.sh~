l7=400
l8=500
l9=300
l10=400

for l1 in 1000 900 800
do
  for l2 in 800 700 600
  do
    for l3 in 600 500 400
    do
      for l4 in 400 300 
      do
        for l5 in 300 200 100
        do
          for l6 in 100 50 20 5
          do
                 echo "Traning $l1,$l2,$l3,$l4,$l5,$l6,$l7,$l8,$l9,$l10 netork"
                 python DNN_train_pass_layers.py $l1,$l2,$l3,$l4,$l5,$l6,$l7,$l8,$l9,$l10

          done
        done
      done
    done
  done
done



